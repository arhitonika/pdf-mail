head
	meta(charset='utf-8')
	meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, minimal-ui')
	meta(http-equiv='X-UA-Compatible' content='IE=edge')

	block head
	title= pageTitle || ''
	meta(name='description' content=pageDescription || '')
	meta(name='keywords' content=pageKeywords || '')
	style.
		* {
		margin: 0;
		padding: 0;
		font: inherit;
		box-sizing: border-box;
		}
		article,
		aside,
		details,
		figcaption,
		figure,
		footer,
		header,
		hgroup,
		main,
		nav,
		section,
		summary {
			display: block;
		}
		audio,
		canvas,
		progress,
		video {
			display: inline-block;
			vertical-align: baseline;
		}
		audio:not([controls]) {
			display: none;
			height: 0;
		}
		[hidden],
		template {
			display: none;
		}
		html {
			font-family: sans-serif;
			-ms-text-size-adjust: 100%;
			-webkit-text-size-adjust: 100%;
			-webkit-tap-highlight-color: rgba(0,0,0,0);
			-webkit-focus-ring-color: rgba(255,255,255,0);
			cursor: default;
		}
		body {
			font-size: 100%;
			line-height: 1;
			-moz-osx-font-smoothing: grayscale;
			-webkit-font-smoothing: antialiased;
		}
		a {
			background: transparent;
		}
		*:focus,
		*:active,
		*:hover {
			outline: none;
		}
		hr {
			box-sizing: content-box;
			height: 0;
		}
		ol,
		ul {
			list-style: none;
		}
		pre {
			-moz-tab-size: 4;
				tab-size: 4;
			white-space: pre-wrap;
		}
		q {
			quotes: '\201C' '\201D' '\2018' '\2019';
		}
		img {
			border: none;
		}
		svg:not(:root) {
			overflow: hidden;
		}
		button,
		input {
			line-height: normal;
		}
		button,
		select {
			text-transform: none;
		}
		button {
			overflow: visible;
		}
		button,
		html input[type='button'],
		input[type='reset'],
		input[type='submit'] {
			-webkit-appearance: button;
			cursor: pointer;
		}
		button[disabled],
		html input[disabled] {
			cursor: default;
		}
		input[type='checkbox'],
		input[type='radio'] {
			box-sizing: border-box;
		}
		input[type='number']::-webkit-inner-spin-button,
		input[type='number']::-webkit-outer-spin-button {
			height: auto;
		}
		input[type='search'] {
			-webkit-appearance: textfield;
			box-sizing: content-box;
		}
		input[type='search']::-webkit-search-cancel-button,
		input[type='search']::-webkit-search-decoration {
			-webkit-appearance: none;
		}
		button::-moz-focus-inner,
		input::-moz-focus-inner {
			border: none;
			padding: 0;
		}
		textarea {
			overflow: auto;
			vertical-align: top;
		}
		button,
		input,
		select[multiple],
		textarea {
			background-image: none;
		}
		input,
		select,
		textarea {
			border-radius: 0;
			box-shadow: none;
		}
		input,
		textarea {
			resize: none;
			-webkit-user-select: text;
				-moz-user-select: text;
					-ms-user-select: text;
						user-select: text;
		}
		[placeholder]:focus::-webkit-input-placeholder {
			color: transparent;
		}
		[placeholder]:focus::-moz-placeholder {
			color: transparent;
		}
		[placeholder]:focus:-ms-input-placeholder {
			color: transparent;
		}
		[placeholder]:focus::placeholder {
			color: transparent;
		}
		table {
			border-collapse: collapse;
			border-spacing: 0;
		}
		.container {
			margin: 0 auto;
			width: 1000px;
			background: #fff;
			box-shadow: 0px 0px 10px rgba(150,150,150,0.2);
		}
		.content {
			padding-top: 44px;
			padding-left: 50px;
			padding-right: 50px;
			width: 1127px;
		}
		.content__title {
			color: #252525;
			font-family: Roboto;
			font-weight: 400;
			line-height: 25px;
			font-size: 36px;
			margin-bottom: 22px;
		}
		.content__subtitle {
			font-size: 24px;
		}
		.footer {
			position: relative;
			padding-top: 84px;
			padding-left: 50px;
			padding-right: 50px;
			width: 1127px;
		}
		.footer:after {
			content: '';
			position: absolute;
			top: 84px;
			left: 50px;
			width: 111px;
			height: 3px;
			background: #ddd;
		}
		.footer__text {
			margin-top: 20px;
			width: 674px;
			height: 76px;
			color: #000;
			font-family: Arial;
			font-size: 13px;
			font-weight: 400;
			line-height: 21px;
		}
		.header {
			width: 1127px;
			height: 108px;
			background-color: #f7f7f7;
			position: relative;
			display: -ms-flexbox;
			display: flex;
			-ms-flex-pack: justify;
			justify-content: space-between;
		}
		.header__home {
			position: relative;
			height: 100%;
		}
		.header__logo {
			background-image: url("data:image/svg+xml;base64,");
			background-repeat: no-repeat;
			background-position: 49px 22px;
			width: 147px;
			height: 100%;
			position: relative;
			float: left;
		}
		.header__logo:before {
			content: '';
			display: block;
			background: #000;
			width: 1px;
			height: 73px;
			opacity: 0.1;
			right: 0;
			top: 19px;
			position: absolute;
		}
		.header__title {
			float: left;
			width: 220px;
			height: 60px;
			color: #252525;
			font-family: Arial;
			font-size: 15px;
			font-weight: 400;
			margin-top: 19px;
			margin-left: 25px;
			position: relative;
			line-height: 23px;
		}
		.header__place {
			width: 414px;
			position: relative;
			height: 100%;
			padding-top: 21px;
			padding-left: 27px;
			color: #252525;
			font-family: Arial;
			font-weight: 400;
		}
		.header__place:before {
			content: '';
			display: block;
			background: #000;
			width: 1px;
			height: 73px;
			opacity: 0.1;
			left: 0;
			top: 19px;
			position: absolute;
		}
		.header__place-title {
			font-size: 15px;
			line-height: 26px;
		}
		.header__place-content {
			font-size: 13px;
			line-height: 21px;
		}
		.header__phone {
			position: relative;
			padding-top: 30px;
		}
		.header__phone-number {
			color: #252525;
			font-family: Arial;
			font-size: 24px;
			font-weight: 400;
		}
		.header__work-time {
			opacity: 0.5;
			color: #252525;
			font-family: Arial;
			font-size: 13px;
			font-weight: 400;
			line-height: 20px;
		}
		.item-box {
			display: -ms-flexbox;
			display: flex;
			height: 115px;
			border: 1px solid #ebebeb;
			border-radius: 5px;
			width: 100%;
			-ms-flex-pack: justify;
			justify-content: space-between;
		}
		.item-box__list-title {
			width: 299px;
			height: 39px;
			font-family: Arial;
			font-weight: 400;
			line-height: 21px;
			padding-top: 31px;
			padding-left: 25px;
		}
		.item-box__list-name {
			color: #252525;
			font-size: 15px;
		}
		.item-box__list-count {
			color: #000;
			font-size: 13px;
		}
		.item-box__product {
			width: 329px;
			padding-top: 20px;
		}
		.item-box__product-image {
			width: 80px;
			height: 80px;
			float: left;
			display: block;
		}
		.item-box__product-description {
			padding-top: 11px;
			padding-left: 22px;
			float: left;
			color: #252525;
			font-family: Arial;
			font-weight: 400;
			width: 170px;
		}
		.item-box__target-name {
			font-size: 15px;
			padding-bottom: 10px;
		}
		.item-box__collection-name {
			font-size: 13px;
			line-height: 21px;
		}
		.item-box__price-box {
			width: 397px;
			height: 41px;
			font-weight: 400;
			padding-top: 29px;
		}
		.item-box__price {
			color: #000;
			font-family: Arial;
			font-size: 18px;
			line-height: 23px;
		}
		.item-box__currency {
			color: #000;
			font-family: ALSRubl, sans-serif;
			font-size: 18px;
			line-height: 23px;
		}
		.item-box__shipment {
			line-height: 30px;
			color: #252525;
			font-family: Arial;
			font-size: 13px;
		}
		body {
			background: #f5f5f5;
		}
		.pr-info {
			padding: 10px 20px;
			margin-bottom: 30px;
		}
		.pr-info__title {
			font-size: 36px;
			margin-bottom: 30px;
		}
		.pr-info__title span {
			font-size: 28px;
		}
		.pr-info__table {
			width: 100%;
			border: 1px solid #ddd;
		}
		.pr-info__table td {
			padding: 20px;
		}
		.pr-info__table img {
			float: left;
			margin-right: 5px;
		}
		.pr-info__table p {
			font-size: 14px;
			margin-bottom: 10px;
		}
		.pr-info__table p:last-child {
			margin-bottom: 0;
		}
		.pr-info__table .pr-info__delivery {
			font-size: 11px;
		}
		.pr-info__table .pr-info__total {
			font-size: 18px;
			font-weight: 700;
		}
		.pr-message {
			padding: 20px 20px 50px;
			width: 70%;
			position: relative;
		}
		.pr-message__text {
			font-size: 14px;
		}
		.pr-table {
			padding: 10px 20px;
			margin-bottom: 30px;
		}
		.pr-table__title {
			font-size: 18px;
			margin-bottom: 20px;
		}
		.pr-table__table {
			width: 100%;
			border-top: 1px solid #ddd;
			border-bottom: 1px solid #ddd;
			font-size: 12px;
		}
		.pr-table__table tr {
			border-bottom: 1px solid #ddd;
		}
		.pr-table__table tr:last-child {
			border-bottom: 0;
		}
		.pr-table__table td {
			padding: 15px 5px;
		}
		.pr-table__warning {
			color: #f00;
		}
		.table__title {
			margin-top: 30px;
			color: #000;
			font-family: Arial;
			font-weight: 400;
			font-size: 15px;
			border-bottom: 1px solid #ebebeb;
			height: 46px;
			line-height: 46px;
		}
		.table__item {
			border-bottom: 1px solid #ebebeb;
			height: 46px;
			line-height: 46px;
			display: -ms-flexbox;
			display: flex;
			-ms-flex-pack: justify;
			justify-content: space-between;
		}
		.table__item-title {
			font-size: 13px;
			width: 400px;
		}
		.table__item-count {
			font-size: 13px;
			width: 200px;
		}
		.table__item-price {
			font-size: 13px;
			width: 185px;
		}
		.table__item-shipment {
			width: 200px;
			font-size: 13px;
		}
		.table__item-shipment_availability {
			color: #ff0303;
		}


