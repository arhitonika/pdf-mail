.header
  width 1127px
  height 108px
  background-color #f7f7f7
  position relative
  display -webkit-box
  display -webkit-flex
  display -ms-flexbox
  display flex
  -webkit-box-pack justify
  -webkit-justify-content space-between
  -ms-flex-pack justify
  justify-content space-between

.header__home
  position relative
  height 100%

.header__logo
  background-image url('data:image/svg+xml;base64,')
  background-repeat no-repeat
  background-position 49px 22px
  width 147px
  height 100%
  position relative
  float left
  &:before
    content ''
    display block
    background #000
    width 1px
    height 73px
    opacity .1
    right 0
    top 19px
    position absolute

.header__title
  float left
  width 220px
  height 60px
  color #252525
  font-family Arial
  font-size 15px
  font-weight 400
  margin-top 19px
  margin-left 25px
  position relative
  line-height 23px

.header__place
  width 414px
  position relative
  height 100%
  padding-top 21px
  padding-left 27px
  color #252525
  font-family Arial
  font-weight 400
  &:before
    content ''
    display block
    background #000
    width 1px
    height 73px
    opacity .1
    left 0
    top 19px
    position absolute

.header__place-title
  font-size 15px
  line-height 26px

.header__place-content
  font-size 13px
  line-height 21px

.header__phone
  position relative
  padding-top 30px

.header__phone-number
  color #252525
  font-family Arial
  font-size 24px
  font-weight 400

.header__work-time
  opacity .5
  color #252525
  font-family Arial
  font-size 13px
  font-weight 400
  line-height 20px
