.pr-table
	padding 10px 20px
	margin-bottom 30px

	&__title
		font-size 18px
		margin-bottom 20px

	&__table
		width 100%
		border-top 1px solid #ddd
		border-bottom 1px solid #ddd
		font-size 12px

		tr
			border-bottom 1px solid #ddd
			&:last-child
				border-bottom 0

		td
			padding 15px 5px

	&__warning
		color red
